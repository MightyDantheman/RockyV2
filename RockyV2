local RS = game:GetService('RunService').RenderStepped
--Other Secret Stuff
function GetCommand(t)
	t = string.sub(t,2)
	if string.find(t,' ') then
		return string.sub(t,1,string.find(t,' ')-1)
	end
end

local CommandFolder = Instance.new('Folder')
CommandFolder.Name = 'Command Folder'

function fct(x)
	for _,v in pairs(x.commands:GetChildren()) do
		if x.Text == GetCommand(v.Text) then
			return true
		end
	end
end

function RemoveSpace(t,b)
	if string.find(t,'	') then
		t = string.sub(t,1,string.find(t,'	')-1)..string.sub(t,string.find(t,'	')+1)
		RemoveSpace(t,b)
	else
		b.Text = t
	end
end

