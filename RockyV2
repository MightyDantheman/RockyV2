ADD_COMMAND('deathtp','deathtp',{},
function(ARGS, SPEAKER)
	DeathTP = true
	if _PLAYERS.LocalPlayer.Character then
		if _PLAYERS.LocalPlayer.Character:FindFirstChild('HumanoidRootPart') then
			DeathPos = _PLAYERS.LocalPlayer.Character.HumanoidRootPart.CFrame
		end
	end
	NOTIFY('DeathTP Enabled',255,255,0)
end)

ADD_COMMAND('undeathtp','undeathtp',{},
function(ARGS, SPEAKER)
	DeathTP = false
	DeathPos = nil
	NOTIFY('DeathTP Disabled',255,255,0)
end)

ADD_COMMAND('fly','fly',{},
function(ARGS, SPEAKER)
	if SPEAKER.Name == game:GetService('Players').LocalPlayer.Name then
		sFLY()
	end
end)

ADD_COMMAND('unfly','unfly',{},
function(ARGS, SPEAKER)
	if SPEAKER.Name == game:GetService('Players').LocalPlayer.Name then
		fly_active = false
		NOFLY()
	end
end)

ADD_COMMAND('prefix','prefix [string]',{},
function(ARGS, SPEAKER)
	if ARGS[1] then
		C_PREFIX = ARGS[1]
		NOTIFY('Changed prefix to \'' .. ARGS[1] .. '\'', 255, 255, 255)
	end
end)

ADD_COMMAND('version','version',{},
function(ARGS, SPEAKER)
	NOTIFY('VERSION | ' .. VERSION, 255, 255, 255)
end)

ADD_COMMAND('fe','fe',{},
function(ARGS, SPEAKER)
	spawn(function()
		CHECK_FE()
	end)
end)

if game.PlaceId == 625747756 then
	ADD_COMMAND('mods','[SCP] mods',{},
	function(ARGS, SPEAKER)
		CHECK_MODS_625747756()
	end)
end

function OPEN_COMMANDS()
	SETH_MAIN.main.holder.Size = UDim2.new(1, 25, 12, 30)
	SETH_MAIN.main.holder.holders.search.Visible = true
end

function CLOSE_COMMANDS()
	SETH_MAIN.main.holder.holders.search.Visible = false
	SETH_MAIN.main.holder.Size = UDim2.new(1, 25, 12, 0)
end

function OPEN_TAB(TAB)
	if not _CORE:FindFirstChild('seth_main') then OPEN_MAIN() end
	for a,b in pairs(SETH_MAIN.main.holder.holders:GetChildren()) do
		if b.Name ~= TAB then
			b.Visible = false
		else
			b.Visible = true
		end
		if TAB ~= 'cmds' then
			CLOSE_COMMANDS()
		else
			OPEN_COMMANDS()
		end
	end
end

ADD_COMMAND('serverinfo','serverinfo',{'sinfo'},
function(ARGS, SPEAKER)
	OPEN_TAB('server')
end)

ADD_COMMAND('admins','admins',{},
function(ARGS, SPEAKER)
	OPEN_TAB('admins')
end)

ADD_COMMAND('cmds','cmds',{'commands'},
function(ARGS, SPEAKER)
	if SPEAKER.Name == game:GetService('Players').LocalPlayer.Name then
		OPEN_TAB('cmds')
	end
end)

ADD_COMMAND('bans','bans',{},
function(ARGS, SPEAKER)
	OPEN_TAB('bans')
end)

ADD_COMMAND('fun','fun',{},
function(ARGS, SPEAKER)
	OPEN_TAB('fun')
end)

ADD_COMMAND('changelog','changelog',{},
function(ARGS, SPEAKER)
	OPEN_TAB('changelog')
end)

ADD_COMMAND('credits','credits',{},
function(ARGS, SPEAKER)
	OPEN_TAB('credits')
end)

MOUSE.KeyDown:connect(function(key)
	if key:byte() == 29 then
		if not NOCLIP then
			ECOMMAND('noclip')
		elseif NOCLIP then
			ECOMMAND('clip')
		end
	elseif key:byte() == 30 then
		if not JESUSFLY then
			ECOMMAND('jesusfly')
		elseif JESUSFLY then
			ECOMMAND('nojfly')
		end
	end
end)

-- / after loaded

function CHECK_FE()
	if not workspace.FilteringEnabled then
		NOTIFY('Filtering is disabled', 50, 255, 50)
	elseif workspace.FilteringEnabled then
		NOTIFY('Filtering is ENABLED', 255, 50, 50)
	end
end

function CHECK_MODS_625747756()
	local i=0
	for _,v in pairs(_PLAYERS:GetChildren()) do
		if v.Character then
			local rank = v.Character.Head:FindFirstChild('Rank')
			if rank then
				if string.find(rank.Frame.TextLabel.Text,'Moderator') then
					i=i+1
					if string.find(rank.Frame.TextLabel.Text,'Head') then
						NOTIFY('Head Mod: '..v.Name, 255, 0, 0)
					else
						NOTIFY('Mod: '..v.Name, 255, 0, 0)
					end
				end
			end
		end
	end
	if i == 0 then
		NOTIFY('No mods found.', 0, 255, 0)
	else
		NOTIFY(i..' total mods', 255, 0, 0)
	end
end

CMD_BAR_H.bar:TweenPosition(UDim2.new(0, 0, 1, -50), 'InOut', 'Quad', 0.5, true)

local GOING_IN = true
CMD_BAR_H.bar.Changed:connect(function()
	if CMD_BAR_H.bar.Text ~= 'press ; to execute a command' and CMD_BAR_H.bar.Focused and not GOING_IN then
		if CMD_BAR_H.bar.Text ~= '' then
			if not CMD_BAR_H.bar.Text:find(' ') then
				CMD_BAR_H.bar.commands.Visible = true
				CMD_BAR_H.bar.commands:ClearAllChildren()
				CMD_BAR_H.bar.commands.CanvasSize = UDim2.new(0, 0, 0, 0)
				local Y_COMMANDS = 0
				for i,v in pairs(COMMANDS) do
					if v.N:find(CMD_BAR_H.bar.Text) then
						CMD_BAR_H.bar.commands:TweenSize(UDim2.new(1, 0, 1, -200), 'InOut', 'Quad', 0.2, true)
						CMD_BAR_H.bar.commands.CanvasSize = CMD_BAR_H.bar.commands.CanvasSize + UDim2.new(0, 0, 0, 20)
						local COMMANDS_C = CMD_BAR_H.bar.commands_ex:Clone()
						COMMANDS_C.Position = UDim2.new(0, 0, -1, Y_COMMANDS)
						COMMANDS_C.Visible = true
						COMMANDS_C.Text = ' ' .. v.D
						COMMANDS_C.Parent = CMD_BAR_H.bar.commands
						Y_COMMANDS = Y_COMMANDS + 20
					end
				end
			end
		else
			CMD_BAR_H.bar.commands:TweenSize(UDim2.new(1, 0, 0, 0), 'InOut', 'Quad', 0.2, true)
			CMD_BAR_H.bar.commands:ClearAllChildren()
			CMD_BAR_H.bar.commands.CanvasSize = UDim2.new(0, 0, 0, 0)
		end
	end
end)

CMD_BAR_H.bar.FocusLost:connect(function()
	CMD_BAR_H.bar.ClearTextOnFocus = true
	GOING_IN = true
	if CMD_BAR_H.bar.Text ~= '' then
		spawn(function()
			ECOMMAND(CMD_BAR_H.bar.Text, LP)
		end)
	end
	CMD_BAR_H.bar.commands:ClearAllChildren()
	CMD_BAR_H.bar.commands.CanvasSize = UDim2.new(0, 0, 0, 0)
	CMD_BAR_H.bar.commands:TweenSize(UDim2.new(1, 0, 0, 0), 'InOut', 'Quad', 0.2, true)
	CMD_BAR_H.bar:TweenPosition(UDim2.new(0, -225, 1, -50), 'InOut', 'Quad', 0.5, true)
end)

MOUSE.KeyDown:connect(function(K)
	if K:byte() == 59 then
		GOING_IN = false
		CMD_BAR_H.bar:TweenPosition(UDim2.new(0, 0, 1, -50), 'InOut', 'Quad', 0.5, true)
		CMD_BAR_H.bar:CaptureFocus()
	end
end)

--NOTIFY('Welcome, ' .. _PLAYERS.LocalPlayer.Name, 255, 255, 255)
--NOTIFY('VERSION | ' .. VERSION, 255, 255, 255)
--CHECK_FE()
function RunIntro()
	local gui = Instance.new('ScreenGui')
	gui.Name = 'RockyIntro'
	local circle = Instance.new('ImageLabel',gui)
	circle.BackgroundTransparency = 1
	circle.Image = 'rbxassetid://3431058390'
	circle.Size = UDim2.new(0.75,0,0)
	circle.ImageColor3 = Color3.new(0,0.5,1)
	local size = circle.AbsoluteSize.X
	local size2 = size+(size*0.2)
	circle.Size = UDim2.new(0,0,0,0)
	circle.Position = UDim2.new(0.5,0,0.5,0)
	local logo = Instance.new('ImageLabel',gui)
	logo.ImageTransparency = 1
	logo.BackgroundTransparency = 1
	logo.Image = 'rbxassetid://3431673710'
	logo.ZIndex = 3
	logo.Size = UDim2.new(0,0,0,0)
	logo.Position = UDim2.new(0.5,0,0.5,0)
	local welcome = Instance.new('TextLabel',gui)
	welcome.ZIndex = 2
	welcome.Size = UDim2.new(0,0,0,0)
	welcome.Position = UDim2.new(0.5,0,0.5,0)
	welcome.TextScaled = true
	welcome.TextWrapped = false
	welcome.TextColor3 = Color3.new(1,1,1)
	welcome.BackgroundTransparency = 1
	welcome.TextSize = 24
	welcome.Text = ''
	local version = Instance.new('TextLabel',gui)
	version.ZIndex = 2
	version.Size = UDim2.new(0,0,0,0)
	version.Position = UDim2.new(0.5,0,0.5,0)
	version.TextWrapped = true
	version.TextScaled = true
	version.Text = 'Version | ' .. VERSION
	version.TextColor3 = Color3.new(1,1,0)
	version.TextTransparency = 1
	version.BackgroundTransparency = 1
	gui.Parent = _CORE
	tween(circle,{Position=UDim2.new(0.5,-size2/2,0.5,-size2/2),Size=UDim2.new(0,size2,0,size2)},0.4,Enum.EasingStyle.Quad):Play()
	wait(0.1)
	tween(logo,{Position=UDim2.new(0.5,-(size2/1.5)/2,0.4,-(size2/4)/2),Size=UDim2.new(0,size2/1.5,0,size2/4),ImageTransparency=0},0.4,Enum.EasingStyle.Quad):Play()
	wait(0.3)
	tween(circle,{Position=UDim2.new(0.5,-size/2,0.5,-size/2),Size=UDim2.new(0,size,0,size)},0.1):Play()
	wait(0.1)
	tween(logo,{Position=UDim2.new(0.5,-(size/1.5)/2,0.4,-(size/4)/2),Size=UDim2.new(0,size/1.5,0,size/4)},0.1,Enum.EasingStyle.Quad):Play()
	tween(version,{Position=UDim2.new(0.5,-(size/1.5)/2,0.65,-(size/6)/2),Size=UDim2.new(0,size/1.5,0,size/6),TextTransparency=0},0.1,Enum.EasingStyle.Quad):Play()
	tween(welcome,{Position=UDim2.new(0.5,-(size/1.5)/2,0.55,-(size/6)/2),Size=UDim2.new(0,(size/1.5),0,size/6)},0.1,Enum.EasingStyle.Quad):Play()
	wait(0.1)
	local s = 'Welcome, '..LP.Name
	for i=1,string.len(s) do
		welcome.Text = welcome.Text..string.sub(s,i,i)
		wait()
	end
	wait(1.5)
	tween(logo,{Position=UDim2.new(0.5,0,0.5,0),Size=UDim2.new(0,0,0,0),ImageTransparency=1},0.4,Enum.EasingStyle.Quad):Play()
	tween(version,{Position=UDim2.new(0.5,0,0.5,0),Size=UDim2.new(0,0,0,0),TextTransparency=1},0.4,Enum.EasingStyle.Quad):Play()
	tween(welcome,{Position=UDim2.new(0.5,0,0.5,0),Size=UDim2.new(0,0,0,0),TextTransparency=1},0.4,Enum.EasingStyle.Quad):Play()
	tween(circle,{Position=UDim2.new(0.5,-size2/2,0.5,-size2/2),Size=UDim2.new(0,size2,0,size2)},0.1,Enum.EasingStyle.Quad):Play()
	wait(0.1)
	tween(circle,{Position=UDim2.new(0.5,0,0.5,0),Size=UDim2.new(0,0,0,0)},0.4,Enum.EasingStyle.Quad):Play()
	wait(0.5)
	gui:Destroy()
end
wait(0.5)
RunIntro()
if game.PlaceId == 625747756 then
	CHECK_MODS_625747756()
end
