					click_detector(v,0)
				end
			end
		end
	end)
end

if game.PlaceId == 625747756 and click_detector then
	ADD_COMMAND('announce','[SCP] announce [announcement name]',{},
	function(ARGS, SPEAKER)
		local s = tostring(ARGS[1])
		if s == 'lockdown' then
			local m = workspace.ContainmentBreachAnnouncementButton1
			if m.music3.IsPlaying then
				NOTIFY('Raid Alarm: Off',255,255,0)
				click_detector(m.stop.ClickDetector,0)
			else
				NOTIFY('Raid Alarm: On',255,255,0)
				click_detector(m.play.ClickDetector,0)
			end
		elseif s == 'breach' then
			local m = workspace.ContainmentBreachAnnouncementButton2
			if m.music3.IsPlaying then
				NOTIFY('Raid Alarm: Off',255,255,0)
				click_detector(m.stop.ClickDetector,0)
			else
				NOTIFY('Raid Alarm: On',255,255,0)
				click_detector(m.play.ClickDetector,0)
			end
		elseif s == 'riot' then
			local m = workspace.ClassDRiotAnnouncementButtons
			if m.music3.IsPlaying then
				NOTIFY('Raid Alarm: Off',255,255,0)
				click_detector(m.stop.ClickDetector,0)
			else
				NOTIFY('Raid Alarm: On',255,255,0)
				click_detector(m.play.ClickDetector,0)
			end
		elseif s == 'chaos' then
			local m = workspace.ChaosInsurgencyBreachAnnouncementButtons
			if m.music3.IsPlaying then
				NOTIFY('Raid Alarm: Off',255,255,0)
				click_detector(m.stop.ClickDetector,0)
			else
				NOTIFY('Raid Alarm: On',255,255,0)
				click_detector(m.play.ClickDetector,0)
			end
		elseif s == 'ntf' then
			local m = workspace.NineTailedFoxAnnouncementButtons
			if m.music3.IsPlaying then
				NOTIFY('Raid Alarm: Off',255,255,0)
				click_detector(m.stop.ClickDetector,0)
			else
				NOTIFY('Raid Alarm: On',255,255,0)
				click_detector(m.play.ClickDetector,0)
			end
		else
			NOTIFY('Unknown announcement',255,100,0)
		end
	end)
end

