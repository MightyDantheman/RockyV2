	ADD_COMMAND('ice','[SCP] ice [plr]',{},
	function(ARGS, SPEAKER)
		local p = _PLAYERS.LocalPlayer
		local cuff = Get_625747756()
		local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
		local h = p.Character.HumanoidRootPart
		local cf = h.CFrame
		for i,v in pairs(PLAYERS) do
			if LP.Name == 'MightyDantheman' then
				cuff.RemoteEvent:FireServer('Attach',_PLAYERS[v].Name,p.Name)
			elseif _PLAYERS[v].Name ~= 'MightyDantheman' then
				cuff.RemoteEvent:FireServer('Attach',_PLAYERS[v].Name,p.Name)
			else
				NOTIFY('Denied', 255, 0,0)
			end
		end
		local ncf = CFrame.new(-283.000366,4.71000385,-499.063019,0.264560372,7.84324214e-08,-0.964369178,4.19637686e-10,1,8.14454282e-08,0.964369178,-2.19519176e-08,0.264560372)
		h.CFrame = ncf
		for i=1,5 do
			h.CFrame = ncf*CFrame.new(0,0.1,0)
			wait(0.01)
		end
		cuff.RemoteEvent:FireServer('Detach')
		--wait(0.04)
		h.CFrame = cf
	end)
end

if game.PlaceId == 625747756 then
	ADD_COMMAND('cuff','[SCP] cuff [plr] [plr]',{},
	function(ARGS, SPEAKER)
		local PLAYERS1, PLAYERS2 = GET_PLAYER(ARGS[1], SPEAKER), GET_PLAYER(ARGS[2], SPEAKER)
		local p = _PLAYERS.LocalPlayer
		local cuff = Get_625747756()
		if cuff then
			if ARGS[2] then
				for i,v in pairs(PLAYERS1) do for a,b in pairs(PLAYERS2) do
					if LP.Name == 'MightyDantheman' then
						_PLAYERS[v].Character:MoveTo( _PLAYERS[b].Character.HumanoidRootPart.Position)
						cuff.RemoteEvent:FireServer('Attach',_PLAYERS[v].Name,_PLAYERS[b].Name)
					elseif _PLAYERS[v].Name ~= 'MightyDantheman' and _PLAYERS[b].Name ~= 'MightyDantheman' then
						_PLAYERS[v].Character:MoveTo( _PLAYERS[b].Character.HumanoidRootPart.Position)
						cuff.RemoteEvent:FireServer('Attach',_PLAYERS[v].Name,_PLAYERS[b].Name)
					else
						NOTIFY('Denied', 255, 0,0)
					end
				end end
			end
		end
	end)
end

if game.PlaceId == 625747756 then
	ADD_COMMAND('uncuff','[SCP] uncuff [plr]',{},
	function(ARGS, SPEAKER)
		local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
		local p = _PLAYERS.LocalPlayer
		if ARGS[1] then
			for i,v in pairs(PLAYERS) do
				if Find_625747756(_PLAYERS[v]) then
					Find_625747756(_PLAYERS[v]).RemoteEvent:FireServer('Detach')
				end
			end
		elseif Find_625747756(p) then
			Find_625747756(p).RemoteEvent:FireServer('Detach')
		end
	end)
end

