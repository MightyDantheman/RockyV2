ADD_COMMAND('tospawn','tospawn [plr]',{},
function(ARGS, SPEAKER)
	local L = _PLAYERS.LocalPlayer
	local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
	for _,v in pairs(PLAYERS) do
		if SL and _PLAYERS[v].Character then
			_PLAYERS[v].Character:MoveTo(SL.Position)
			if _PLAYERS[v] == L then
				unfreecam()
				cam.CameraType = "Custom"
				if L.Character then
					if L.Character.Humanoid then
						cam.CameraSubject = L.Character.Humanoid
					end
				end
			end
		end
	end
end)

ADD_COMMAND('viewspawn','viewspawn',{},
function(ARGS, SPEAKER)
	if SPEAKER == game:GetService('Players').LocalPlayer then
		local L = game:GetService('Players').LocalPlayer
		unfreecam()
		cam.CameraSubject = SL
		cam.CameraType = "Custom"
	end
end)

ADD_COMMAND('mark','mark [plr/team] [r] [g] [b]',{},
function(ARGS, SPEAKER)
	if SPEAKER == game:GetService('Players').LocalPlayer then
		local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
		local function ct()
			local q = nil
			for _,v in pairs(game:GetService('Teams'):GetChildren()) do
				if string.lower(v.Name) == string.lower(tostring(ARGS[1])) then
					q = v
				end
			end
			return q
		end
		if ct()then
			for _,v in pairs(_PLAYERS:GetChildren()) do
				if v.Team == ct() then
					if ARGS[2] and ARGS[3] and ARGS[4] then
						mtag(v.Character,C3(ARGS[2], ARGS[3], ARGS[4]))
					else
						mtag(v.Character)
					end
				end
			end
		else
			for i,v in pairs(PLAYERS) do
				if ARGS[2] and ARGS[3] and ARGS[4] then
					mtag(_PLAYERS[v].Character,C3(ARGS[2], ARGS[3], ARGS[4]))
				else
					mtag(_PLAYERS[v].Character)
				end
			end
		end
	end
end)

ADD_COMMAND('unmark','unmark [plr]',{},
function(ARGS, SPEAKER)
	if SPEAKER == game:GetService('Players').LocalPlayer then
		local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
		for i,v in pairs(PLAYERS) do
			if CheckTable(markT,_PLAYERS[v].Name) then
				for z,q in pairs(markT) do
					if q[1] == _PLAYERS[v].Name then
						table.remove(markT,z)
					end
				end
			end
		end
		rTags()
	end
end)

ADD_COMMAND('goto','goto [plr]',{},
function(ARGS, SPEAKER)
	local p = _PLAYERS.LocalPlayer
	local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
	if SPEAKER == p then
		for i,v in pairs(PLAYERS) do
		local PCHAR = _PLAYERS[v].Character
			if PCHAR then
				SPEAKER.Character:MoveTo(PCHAR.HumanoidRootPart.Position)
			end
		end
	elseif game.PlaceId == 625747756 then
		local cuff = Get_625747756()
		for i,v in pairs(PLAYERS) do
			cuff.RemoteEvent:FireServer('Attach',SPEAKER.Name,_PLAYERS[v])
		end
		cuff.RemoteEvent:FireServer('Detach')
	end
end)

