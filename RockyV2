if game.PlaceId == 625747756 then
	ADD_COMMAND('team','[SCP] team [team name]',{},
	function(ARGS, SPEAKER)
		local function find()
			for _,v in pairs(_Teams:GetChildren()) do
				if string.find(string.lower(v.Name),string.lower(ARGS[1])) then
					return v
				end
			end
		end
		local team = find()
		if team then
			_ReplicatedStorage.RemoteFunctionIntroductionManager:InvokeServer('SwitchTeam',team.Name)
			if LP.Team ~= team then
				NOTIFY('Team restricted', 255, 0, 0)
			else
				local cf = LP.Character.HumanoidRootPart.CFrame
				LP.Character:BreakJoints()
				LP.CharacterAdded:Wait()
				LP.Character:WaitForChild('HumanoidRootPart').CFrame = cf
			end
		else
			NOTIFY('Team not found', 255, 100, 0)
		end
	end)
end

if game.PlaceId == 625747756 then
	ADD_COMMAND('uniform','[SCP] uniform [rank (ex:IA,ScD)]',{},
	function(ARGS, SPEAKER)
		local clothes = {
		IA = 219287347,
		SP = 303228734,
		SCD = 329020421,
		MD = 331447569,
		CI = 462501371}
		local morphs = {
		SD =  workspace['Security-Personnel'].button,
		MTF = workspace['Mobile-Task-Force'].button,
		A1 = workspace['Mobile-Task-Force A-1'].button,
		NTF = workspace['Nine-Tailed-Fox'].button,
		CI = workspace['    '].Head}
		local rank = string.upper(ARGS[1])
		local function find(t,s) if t[s] then return t[s] end end
		local A1 = find(clothes,rank)
		local A2 = find(morphs,rank)
		if A1 then
			if click_detector then
				for _,v in pairs(workspace:GetChildren()) do
					if v.Name == 'Scientist uniform' and v:FindFirstChild('ClickPart') then
						if string.find(v.Coathanger.Shirt.ShirtTemplate,A1) then
							click_detector(v.ClickPart.ClickDetector,0)
						end
					end
				end
			else
				NOTIFY('Missing: click_detector API', 255, 100, 0)
			end
		end
		if A2 or rank == 'VEST' then
			if rank == 'VEST' then
				A2 = morphs.CI
			end
			local cf = A2.CFrame
			local trans = A2.Transparency
			local col = A2.CanCollide
			A2.CanCollide = false
			A2.Transparency = 1
			for i=1,5 do
				A2.CFrame = LP.Character.HumanoidRootPart.CFrame
				wait()
			end
			A2.CFrame = cf
			A2.Transparency = trans
			A2.CanCollide = col
		end
	end)
end

if game.PlaceId == 625747756 then
	ADD_COMMAND('sd','[SCP] sd',{},
	function(ARGS, SPEAKER)
		local remote = _ReplicatedStorage.RemoteFunctionIntroductionManager
		local loc = CFrame.new(-33,10,-370)
		local function team(n) remote:InvokeServer('SwitchTeam',n) end
		local function count() return #LP.Backpack:GetChildren() end
		local cf = LP.Character.HumanoidRootPart.CFrame
		local tools = {}
		team('Security Department')
		LP.Character:BreakJoints()
		LP.CharacterAdded:Wait()
		LP.Character:WaitForChild('HumanoidRootPart').CFrame = loc
		repeat wait() until count() >= 5
		for _,v in pairs(LP.Backpack:GetChildren()) do
			table.insert(tools,v)
			v.Parent = LP.Character
			v.Parent = workspace
		end
		LP.Character.HumanoidRootPart.CFrame = loc*CFrame.new(0,0,10)
		team('D Class Personnel')
		LP.Character:BreakJoints()
		LP.CharacterAdded:Wait()
		LP.CharacterAdded:Wait()
		LP.Character:WaitForChild('HumanoidRootPart').CFrame = cf
		LP.Character.HumanoidRootPart.Anchored = true
		for _,v in pairs(tools) do
			local handle = v:FindFirstChild('Handle')
			if handle then
				local i=0
				repeat
					i=i+1
					LP.Character.HumanoidRootPart.CFrame = handle.CFrame
					wait()
				until handle.Parent.Parent ~= workspace or i >= 100
			end
		end
		wait()
		for _,v in pairs(LP.Character:GetChildren()) do
			if v:IsA('Tool') then
				v.Parent = LP.Backpack
			end
		end
		LP.Character.HumanoidRootPart.Anchored = false
		LP.Character.HumanoidRootPart.CFrame = cf
	end)
end

