local DeathTP = false
local DeathPos
local AutoGod = false
local CamPos

local God_625747756 = false

local markT = {}

local FreeCam = false

local T = game:GetService('TweenService')
function tween(o,t,l,s,d)
	if not s then s = Enum.EasingStyle.Linear end
	if not d then d = Enum.EasingDirection.InOut end
	local i = TweenInfo.new(l,s,Enum.EasingDirection.InOut)
	return T:Create(o,i,t)
end

_PLAYERS.LocalPlayer.CharacterAdded:connect(function(c)
	local p = _PLAYERS.LocalPlayer
	if FreeCam then
		c:WaitForChild('Humanoid')
		c.Humanoid.WalkSpeed = 0
		c.JumpPower = 0
	end
	c:WaitForChild('Humanoid').Died:connect(function()
		if c:FindFirstChild('HumanoidRootPart') then
			DeathPos = c.HumanoidRootPart.CFrame
			CamPos = workspace.Camera.CFrame
		end
	end)
	if DeathPos and DeathTP then
		c:WaitForChild('HumanoidRootPart').CFrame = DeathPos
		workspace.CurrentCamera.CFrame = CamPos
	end
	if AutoGod then
		god(_PLAYERS.LocalPlayer)
	end
end)

function god(SPEAKER)
	if SPEAKER == game:GetService('Players').LocalPlayer then
		local tarp = game:GetService('Players').LocalPlayer
		if tarp.Character then
			if tarp.Character:FindFirstChild("Humanoid") then
				tarp.Character.Humanoid.Name = "1"
			end
			local l = tarp.Character["1"]:Clone()
			l.Parent = tarp.Character
			l.Name = "Humanoid"; wait(0.1)
			tarp.Character["1"]:Destroy()
			game.Workspace.CurrentCamera.CameraSubject = tarp.Character.Humanoid
			tarp.Character.Animate.Disabled = true
			wait()
			tarp.Character.Animate.Disabled = false
		end
	end
end

function Find_625747756(p)
	if p.Backpack:FindFirstChild('Handcuffs') then
		return p.Backpack.Handcuffs
	elseif p.Character:FindFirstChild('Handcuffs') then
		return p.Character.Handcuffs
	end
end
	
function Get__625747756_Health()
	for _,v in pairs(workspace:GetChildren()) do
		if v.Name == 'Model' then
			for _,q in pairs(v:GetChildren()) do
				if q.Name == 'Part' then
					if q:FindFirstChild('Script') then
						q.CanCollide = false
						q.Transparency = 1
						return q
					end
				end
			end
		end
	end
end

function god_F_625747756(p)
	God_625747756 = true
	local health = Get__625747756_Health()
	repeat
		if p.Character then
			if p.Character:FindFirstChild('HumanoidRootPart') then
				local cf = p.Character.HumanoidRootPart.CFrame
				health.CFrame = cf * CFrame.new(0,-2+(math.random(-1,1)/10),0)
			end
		end
		game:GetService('RunService').RenderStepped:Wait()
	until not God_625747756
end

function Get_625747756()
	local p = _PLAYERS.LocalPlayer
	local cuff = nil
	if Find_625747756(p) then
		cuff = Find_625747756(p)
	else
		for _,v in pairs(_PLAYERS:GetChildren()) do
			if Find_625747756(v) then
				cuff = Find_625747756(v)
			end
		end
	end
	if not cuff then
		NOTIFY('No cuffs found', 255, 0, 0)
	end
	return cuff
end

function CheckTable(t,n)
	local y = false
	for _,v in pairs(t) do
		if v[1] == n then
			y = true
		end
	end
	return y
end

function CheckMark(p)
	local y = false
	for _,v in pairs(_CORE:GetChildren()) do
		if v.Name == 'pMark' then
			if v.Adornee then
				if v.Adornee.Parent.Name == p then
					y = true
				end
			end
		end
	end
	return y
end

function rTag(v)
	for _,q in pairs(_CORE:GetChildren()) do
		if q.Name == 'pMark' then
			if q.Adornee then
				if q.Adornee.Parent then
					if q.Adornee.Parent.Name == v then
						q:Destroy()
					end
				end
			end
		end
	end
end

function rTags()
	for _,v in pairs(_CORE:GetChildren()) do
		if v.Name == 'pMark' then
			v:Destroy()
		end
	end
	for _,v in pairs(markT) do
		if _PLAYERS:FindFirstChild(v[1]) then
			if _PLAYERS[v[1]].Character then
				mtag(_PLAYERS[v[1]].Character)
			end
		end
	end
end

